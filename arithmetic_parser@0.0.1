#include<iostream>

using namespace std;
int sum(int x, int y)
{
   return x+y;
}
int meh(int x, int y)
{
   return x-y;
}
int sleh(int x, int y)
{
   return x*y;
}
int keh(int x, int y)
{
   return x/y;
}
int annd(int x, int y)
{
   return x%y;
}
int akbar(int x, int y)
{
   return x&y;
}
int xlopec(int x, int y)
{
   return x|y;
}
int iq(int x, int y)
{
   return x^y;
}
int left(int x, int y)
{
   return x<<y;
}
int right(int x, int y)
{
   return x>>y;
}
int main()
{
   int x,y,res;
   
   char op,op1;
   

    
   cin>> x;
   cin>> op;
   if (op=='>') {cin>>op1;}
   else if(op=='<') {cin>>op1;}
  cin>>y;
 if(op == '+')     
   {
       
       
       res=sum(x,y);
       cout<< res <<endl;
   }
 else if(op == '-')
 {
    
     res=meh(x,y);
     cout<<res<<endl;
 }

 else if(op == '*')
 {
  
     
     res=sleh(x,y);
     cout<<res<<endl;
 }
 else if(op =='/')
 {
     if(y==0 && x!=0)
   {cout<<"inf";}
    if(y==0 && x==0)
    {cout<<"nan";}
    if(y>0)
    {res=keh(x,y);
   cout<<res;}
   if(y<0)
    {res=keh(x,y);
   cout<<res;}

  
    
    
 }
 else if(op =='%')
 {
   
     res=annd(x,y);
     cout<<res<<endl;
 }
 else if(op =='&')
 { 
     res=akbar(x,y);
     cout<<res<<endl;
 }
else if(op =='|')
{    
     res=xlopec(x,y);
     cout<<res<<endl;
}
else if(op =='^')
{   
     res=iq(x,y);
     cout<<res<<endl;
}
else  if (op =='<')
 {
    
     res=left(x,y);
     cout<<res<<endl;
 }
else if (op =='>')
{
    
    res=right(x,y);
    cout<<res<<endl;
}
    cin.get();
   return 0;


}
